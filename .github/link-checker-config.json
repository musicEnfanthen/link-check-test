{
  "ignorePatterns": [
        {
            "pattern": "^(http|https)://localhost"
        },
	{
    	    "pattern": "mailto:{{.*}}"
  	},
        {
	    "pattern": "https://github.com/{{.*}}/actions/runs/{{.*}}"
	}
    ],
  "replacementPatterns": [
   	{
	     "pattern": "^/",
	     "replacement": "https://music-encoding.org/"
	},
   	{
	     "pattern": "^../schema/",
	     "replacement": "https://music-encoding.org/schema/"
	},
	{
	     "pattern": "^../102_march.png",
	     "replacement": "./102_march.png"
	}
  ],
  "retryOn429": true,
  "aliveStatusCodes": [200, 206]
}
